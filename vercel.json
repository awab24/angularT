{
    "builds": [
      {
        "src": "contact-management/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "contact-management/dist/contact-management"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/contact-management/index.html"
      }
    ]
  }
  
  