{
    "version": 2,
    "builds": [
      {
        "src": "dist/contact-management/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/dist/contact-management/index.html" },
      { "handle": "filesystem" }
    ],
    "error": {
      "src": "/404.html",
      "status": 404
    }
  }
  